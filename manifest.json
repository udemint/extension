{
  "manifest_version": 3,
  "name": "Udemint Helper",
  "version": "1.0.0",
  "description": "Bypasses browser CORS restrictions for Udemint and its worker domains.",
  "author": "Udemint",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
